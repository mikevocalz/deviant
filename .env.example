# Apple Development Team ID (Micah) - for iOS Release builds
# Find in Xcode: Preferences > Accounts > select team, or developer.apple.com
APPLE_DEVELOPMENT_TEAM_ID=

# Google Maps & Places API Keys
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Enable these APIs: Maps SDK for Android, Maps SDK for iOS, Places API
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
EXPO_PUBLIC_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Payload CMS (Server-only - NEVER prefix with EXPO_PUBLIC_)
# These are only accessible in API routes, not client code
PAYLOAD_URL=http://localhost:3000
PAYLOAD_API_KEY=your_payload_api_key_here

# Better Auth
BETTER_AUTH_SECRET=your-better-auth-secret
NEXT_PUBLIC_BETTER_AUTH_URL=http://localhost:8081
EXPO_PUBLIC_AUTH_URL=http://localhost:8081
DATABASE_URI=postgresql://user:password@localhost:5432/deviant

# Production API URL (required for native app builds)
# This is where your Expo Router API routes are deployed (e.g., Vercel)
# Leave empty for development to use relative URLs with Expo dev server
EXPO_PUBLIC_API_URL=

# Bunny.net CDN (Media Uploads)
EXPO_PUBLIC_BUNNY_STORAGE_ZONE=your-storage-zone-name
EXPO_PUBLIC_BUNNY_STORAGE_API_KEY=your-storage-api-key
EXPO_PUBLIC_BUNNY_STORAGE_REGION=ny
EXPO_PUBLIC_BUNNY_CDN_URL=https://your-zone.b-cdn.net

# Internal API Key (for Payload hooks to call send-notification endpoint)
INTERNAL_API_KEY=your-internal-api-key-here

# Fishjam Video (Client - safe to expose, just an app identifier)
EXPO_PUBLIC_FISHJAM_APP_ID=your-fishjam-app-id

# Social Providers (optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
APPLE_CLIENT_ID=
APPLE_CLIENT_SECRET=

# Stripe (Ticketing + Sneaky Link Paywall)
# Client-side publishable key (safe to embed in app)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
# Set in Supabase secrets for Edge Functions, not client-side
# STRIPE_SECRET_KEY=sk_test_...
# STRIPE_WEBHOOK_SECRET=whsec_...
# STRIPE_PUBLISHABLE_KEY=pk_test_... (also set in Supabase secrets for PaymentSheet)

# MapTiler (MapLibre GL maps)
# Get your API key from: https://cloud.maptiler.com/account/keys
EXPO_PUBLIC_MAPTILER_KEY=your_maptiler_api_key_here
# Optional: override the default dark style
# EXPO_PUBLIC_MAPTILER_STYLE_URL=https://api.maptiler.com/maps/streets-v2-dark/style.json

# Feature Flags (all default OFF — set to "true" or "1" to enable)
EXPO_PUBLIC_FF_EVENTS_ENABLED=false
EXPO_PUBLIC_FF_EVENT_WEATHER_ENABLED=false
EXPO_PUBLIC_FF_TICKETING_ENABLED=false
EXPO_PUBLIC_FF_ORGANIZER_TOOLS_ENABLED=false
EXPO_PUBLIC_FF_PAYOUTS_ENABLED=false
EXPO_PUBLIC_FF_SNEAKY_PAYWALL_ENABLED=false
EXPO_PUBLIC_FF_NSA_ENABLED=false
EXPO_PUBLIC_FF_MAPS_ENABLED=false

# Performance Feature Flags (all default OFF — enable to test perf improvements)
EXPO_PUBLIC_FF_PERF_BOOTSTRAP_FEED=false
EXPO_PUBLIC_FF_PERF_BOOTSTRAP_PROFILE=false
EXPO_PUBLIC_FF_PERF_BOOTSTRAP_MESSAGES=false
EXPO_PUBLIC_FF_PERF_BOOTSTRAP_NOTIFICATIONS=false
EXPO_PUBLIC_FF_PERF_BOOTSTRAP_EVENTS=false
EXPO_PUBLIC_FF_PERF_PREFETCH_ROUTER=false
EXPO_PUBLIC_FF_PERF_INSTRUMENTATION=true
