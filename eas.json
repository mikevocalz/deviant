{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_PUBLIC_API_URL": "${API_URL}",
        "EXPO_PUBLIC_AUTH_URL": "${API_URL}",
        "EXPO_PUBLIC_BUNNY_STORAGE_ZONE": "${BUNNY_STORAGE_ZONE}",
        "EXPO_PUBLIC_BUNNY_STORAGE_API_KEY": "${BUNNY_STORAGE_API_KEY}",
        "EXPO_PUBLIC_BUNNY_STORAGE_REGION": "${BUNNY_STORAGE_REGION}",
        "EXPO_PUBLIC_BUNNY_CDN_URL": "${BUNNY_CDN_URL}"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_API_URL": "${API_URL}",
        "EXPO_PUBLIC_AUTH_URL": "${API_URL}",
        "EXPO_PUBLIC_BUNNY_STORAGE_ZONE": "${BUNNY_STORAGE_ZONE}",
        "EXPO_PUBLIC_BUNNY_STORAGE_API_KEY": "${BUNNY_STORAGE_API_KEY}",
        "EXPO_PUBLIC_BUNNY_STORAGE_REGION": "${BUNNY_STORAGE_REGION}",
        "EXPO_PUBLIC_BUNNY_CDN_URL": "${BUNNY_CDN_URL}"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "${API_URL}",
        "EXPO_PUBLIC_AUTH_URL": "${API_URL}",
        "EXPO_PUBLIC_BUNNY_STORAGE_ZONE": "${BUNNY_STORAGE_ZONE}",
        "EXPO_PUBLIC_BUNNY_STORAGE_API_KEY": "${BUNNY_STORAGE_API_KEY}",
        "EXPO_PUBLIC_BUNNY_STORAGE_REGION": "${BUNNY_STORAGE_REGION}",
        "EXPO_PUBLIC_BUNNY_CDN_URL": "${BUNNY_CDN_URL}"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6758054072"
      }
    }
  }
}
