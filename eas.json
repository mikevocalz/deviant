{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "image": "latest"
      },
      "env": {
        "APP_ENV": "development",
        "DATABASE_URL": "${DATABASE_URL}",
        "EXPO_PUBLIC_API_URL": "${API_URL}",
        "EXPO_PUBLIC_AUTH_URL": "https://npfjanxturvmjyevoyfo.supabase.co/functions/v1/auth",
        "EXPO_PUBLIC_BUNNY_STORAGE_ZONE": "${BUNNY_STORAGE_ZONE}",
        "EXPO_PUBLIC_BUNNY_STORAGE_REGION": "${BUNNY_STORAGE_REGION}",
        "EXPO_PUBLIC_BUNNY_CDN_URL": "${BUNNY_CDN_URL}",
        "EXPO_PUBLIC_FISHJAM_APP_ID": "${FISHJAM_APP_ID}",
        "EXPO_PUBLIC_SUPABASE_URL": "${EXPO_PUBLIC_SUPABASE_URL}",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "${EXPO_PUBLIC_SUPABASE_ANON_KEY}",
        "EXPO_PUBLIC_KLIPY_API_KEY": "${EXPO_PUBLIC_KLIPY_API_KEY}",
        "RESEND_API_KEY": "${RESEND_API_KEY}",
        "RESEND_FROM_EMAIL": "${RESEND_FROM_EMAIL}",
        "EXPO_PUBLIC_MAPTILER_KEY": "${MAPTILER_KEY}"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "production",
      "ios": {
        "image": "latest"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "preview",
        "DATABASE_URL": "${DATABASE_URL}",
        "EXPO_PUBLIC_API_URL": "${API_URL}",
        "EXPO_PUBLIC_AUTH_URL": "https://npfjanxturvmjyevoyfo.supabase.co/functions/v1/auth",
        "EXPO_PUBLIC_BUNNY_STORAGE_ZONE": "${BUNNY_STORAGE_ZONE}",
        "EXPO_PUBLIC_BUNNY_STORAGE_REGION": "${BUNNY_STORAGE_REGION}",
        "EXPO_PUBLIC_BUNNY_CDN_URL": "${BUNNY_CDN_URL}",
        "EXPO_PUBLIC_FISHJAM_APP_ID": "${FISHJAM_APP_ID}",
        "EXPO_PUBLIC_SUPABASE_URL": "${EXPO_PUBLIC_SUPABASE_URL}",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "${EXPO_PUBLIC_SUPABASE_ANON_KEY}",
        "EXPO_PUBLIC_KLIPY_API_KEY": "${EXPO_PUBLIC_KLIPY_API_KEY}",
        "RESEND_API_KEY": "${RESEND_API_KEY}",
        "RESEND_FROM_EMAIL": "${RESEND_FROM_EMAIL}",
        "EXPO_PUBLIC_MAPTILER_KEY": "${MAPTILER_KEY}"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "ios": {
        "image": "latest"
      },
      "env": {
        "APP_ENV": "production",
        "DATABASE_URL": "${DATABASE_URL}",
        "EXPO_PUBLIC_API_URL": "${API_URL}",
        "EXPO_PUBLIC_AUTH_URL": "https://npfjanxturvmjyevoyfo.supabase.co/functions/v1/auth",
        "EXPO_PUBLIC_BUNNY_STORAGE_ZONE": "${BUNNY_STORAGE_ZONE}",
        "EXPO_PUBLIC_BUNNY_STORAGE_REGION": "${BUNNY_STORAGE_REGION}",
